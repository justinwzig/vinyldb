/* Main container styling */
main, footer {
    /* border-color: var(--yellow);
    border-width: 2px;
    border-style: solid; */
    margin: 20px;
    padding: 20px;
    border-radius: 0;
}

nav {
    justify-content: center;
    box-sizing: border-box;
    border-style: solid;
    border-color: var(--yellow);
    display: inline-flex;
    position: relative;
    overflow: hidden;
    padding: 0 20px;
    max-width: 100%;
    background-color: var(--dark03);
    border-radius: 40px;
}

nav {
    a {
        color: var(--base1);
        padding: 20px;
        text-decoration: none;
        transition: 0.3s;
        margin: 0 6px;
        z-index: 1;
        font-weight: 500;
        position: relative;

        &:before {
            content: '';
            position: absolute;
            bottom: -6px;
            left: 0;
            width: 100%;
            height: 5px;
            background-color: var(--base2);
            border-radius: 8px 8px 0 0;
            opacity: 0;
            transition: 0.3s;
        }
    }

    :not(.active):hover:before {
        opacity: 1;
        bottom: 0;
    }

    :not(.active):hover {
        color: var(--base3);
    }
}

.nav-indicator {
    position: absolute;
    left: 0;
    bottom: 0;
    transition: 0.4s;
    height: 5px;
    z-index: 1;
    border-radius: 8px 8px 0 0;
}

@media (max-width: 580px) {
    .nav {
        overflow: auto;
    }
}

.row, .col-2 {
    flex: nowrap;
}

.box {
    box-sizing: border-box;
    flex: 1 1 auto;
    color: #fff;
    border-radius: 5px;
    padding: 20px;
    font-size: 150%;
}

[class*='handle'] {
    width: 5px;
    padding: 0;
    cursor: col-resize;
    flex: 0 0 auto;
    z-index: 100;
}

.handler:hover {
    cursor: col-resize;
}

.handler::before {
    content: '';
    display: block;
    width: 4px;
    height: 100%;
    background: red;
    margin: 0 auto;
}

h5 {
    color: #fff;
}

a, a:hover, .icon {
    height: 1.5em;
    width: 1.5em;
    color: light03;
}

.row > .icon {
    align-self: center;
    display: flex;
    margin: 4px 8px;
}

.toc {
    width: var(--menu-width);
    line-height: 2;

    ul {
        list-style: none;
        padding: 0;
        margin: 0;
    }

    ul ul {
        padding-left: 2em;
    }

    li a {
        display: inline-block;
        color: #aaa;
        text-decoration: none;
        transition: all 0.3s cubic-bezier(0.23, 1, 0.32, 1);
    }

    li.visible>a {
        color: var(--base3);
        transform: translate(5px);
    }
}

.toc-marker {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: -1;
    color: var(--base3);

    path {
        transition: all 0.3s ease;
    }
}



